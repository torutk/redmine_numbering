<h2><%=h l(:label_numbering_item) %>#<%= @numbering_item.id %></h2>

<%= labelled_form_for :numbering_item, @numbering_item,
    :url => project_numbering_item_path(@project, :numbering_prefix_id => @numbering_prefix),
    :html => {:multipart => true, :id => 'numbering_item_form'} do |f| %>
  <%= render :partial => 'numbering_items/form', :locals => {:form => f} %>
  <%= f.submit l(:button_edit) %>
<% end %>

