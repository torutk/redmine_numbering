<h2><%=h l(:label_numbering) %>#<%= @numbering_prefix.id %></h2>

<%= labelled_form_for :numbering_prefix, @numbering_prefix,
    {:url => project_numbering_prefix_path(@project),
     :html => {:multipart => true, :id => 'numbering_prefix_form'}} do |p| %>
  <%= render :partial => 'numbering_prefixes/form', :locals => {:form => p} %>
  <%= p.submit l(:button_edit) %>
<% end %>
